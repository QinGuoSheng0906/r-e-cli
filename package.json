{
  "name": "c-s-cli",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./service/bin/www",
    "dev": "cross-env PROCESS_ENV=development webpack-dev-server --progress  --config  ./config/webpack/dev.js",
    "ap": "cross-env PROCESS_ENV=acceptance webpack --progress --config ./config/webpack/build.js",
    "pre": "cross-env PROCESS_ENV=preannouncement webpack --progress --config ./config/webpack/build.js",
    "pro": "cross-env PROCESS_ENV=production webpack --progress --config ./config/webpack/build.js",
    "build": "cross-env PROCESS_ENV=production webpack --progress  --config ./config/webpack/build.js",
    "watch": "cross-env PROCESS_ENV=watch  webpack --progress  --config ./config/webpack/build.js",
    "server": "concurrently \"npm start\" \"npm run watch\"",
    "lint": "eslint --ext .js --ext .jsx client",
    "lint:style": "stylelint --fix client/**/*.{css,less} "
  },
  "pre-commit": [
    "lint"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
