{
   "compilerOptions": {
     "baseUrl": "./",
     "paths": {
       "@/*": ["client/*"],
       "@components/*": ["client/components/*"],
       "@actions/*": ["client/stores/actions/*"],
       "@assets/*": ["client/assets/*"],
       "@images/*": ["client/assets/images/*"],
       "@lib/*": ["client/lib/*"],
       "@enums/*": ["client/lib/enums*"],
       "@pages/*": ["client/pages/*"],
     },
     "target": "ES6",
     "module": "commonjs",
     "jsx": "react",
     "allowSyntheticDefaultImports": true
   },
   "include": [
     "client/**/*"
   ],
   "exclude": ["node_modules"]
}
